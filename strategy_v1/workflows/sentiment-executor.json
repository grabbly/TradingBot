    {
      "parameters": {
        "jsCode": "const acct = $items('Alpaca-get-account-info')[0]?.json || {};\nconst equity = Number(acct.equity || acct.last_equity || 0);\nconst maxBal = Number($json.max_balance || 0);\n\nif (!equity || !maxBal) {\n  return { json: { skip_trading: false, drawdown: 0, equity, max_balance: maxBal } };\n}\n\nconst drawdown = (maxBal - equity) / maxBal;\nconst skip = drawdown >= 0.10;\n\nreturn { json: { skip_trading: skip, drawdown, equity, max_balance: maxBal } };"
      },
