    {
      "parameters": {
        "jsCode": "const openPositions = $('Alpaca_get_open_positions').all();\nconst topSentiment = $('filter_top_sentiment_score').all();\nconst account = $('Alpaca-get-account-info').first()?.json ?? {};\n\n// –û—Ç—Ñ–∏–ª—å—Ç—Ä–æ–≤–∞—Ç—å –ø—É—Å—Ç—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã (–∫–æ—Ç–æ—Ä—ã–µ –ø–æ—è–≤–ª—è—é—Ç—Å—è –ø—Ä–∏ alwaysOutputData, –∫–æ–≥–¥–∞ –ø–æ–∑–∏—Ü–∏–π –Ω–µ—Ç)\nconst validOpenPositions = openPositions.filter(pos => pos.json && pos.json.symbol);\n\nconst openPositionSymbols = validOpenPositions.map(pos => pos.json.symbol);\nconst topSentimentSymbols = topSentiment.map(sen => sen.json.stock);\n\nconst positionsNotInTopSentiment = validOpenPositions\n  .filter(pos => !topSentimentSymbols.includes(pos.json.symbol))\n  .map(pos => ({\n    symbol: pos.json.symbol,\n    market_value: pos.json.market_value,\n    qty: pos.json.qty,\n    asset_id: pos.json.asset_id\n  }));\n\nconst symbolsNotInOpenPositions = topSentiment\n  .filter(sen => !openPositionSymbols.includes(sen.json.stock))\n  .map(sen => ({\n    symbol: sen.json.stock,\n  }));\n\nconst totalMarketValue = positionsNotInTopSentiment.reduce((sum, pos) => sum + (parseFloat(pos.market_value) || 0), 0);\n\n// V2: –µ—Å–ª–∏ –ø—Ä–æ–¥–∞–≤–∞—Ç—å –Ω–µ—á–µ–≥–æ (–ø–æ—Ä—Ç—Ñ–µ–ª—å –ø—É—Å—Ç–æ–π –∏–ª–∏ —É–∂–µ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É–µ—Ç —Ç–æ–ø-4), –∏—Å–ø–æ–ª—å–∑—É–µ–º —Å–≤–æ–±–æ–¥–Ω—ã–µ –¥–µ–Ω—å–≥–∏ –¥–ª—è –ø–µ—Ä–≤–æ–π –ø–æ–∫—É–ø–∫–∏\nconst availableCash = parseFloat(account.cash ?? account.buying_power ?? '0') || 0;\nconst equity = parseFloat(account.equity ?? account.last_equity ?? availableCash) || 0;\nconst totalBudget = totalMarketValue > 0 ? totalMarketValue : availableCash;\n\n// Risk management (Phase 4)\nconst stopLossPercent = 0.02; // 2% SL\nconst takeProfitPercent = 0.04; // 4% TP (2:1)\n\n// Fixed position size: $500 per symbol (min for expensive stocks like AAPL ~$280)\nconst valuePerSymbol = 500;\n\n// Old percentage-based logic (disabled):\n// const riskPercent = 0.01; // 1% risk per trade\n// const maxPositionPercent = 0.25; // cap 25% of equity\n// const riskDollars = equity * riskPercent;\n// const baseNotional = stopLossPercent > 0 ? (riskDollars / stopLossPercent) : 0;\n// const maxNotional = equity * maxPositionPercent;\n// const perSymbolBudget = symbolsNotInOpenPositions.length > 0\n//   ? totalBudget / symbolsNotInOpenPositions.length\n//   : 0;\n// const valuePerSymbol = Math.max(0, Math.min(baseNotional, maxNotional, perSymbolBudget));\n\nconst symbolsWithMarketValue = symbolsNotInOpenPositions.map(item => ({\n  ...item,\n  market_value_per_symbol: valuePerSymbol,\n  stop_loss_percent: stopLossPercent,\n  take_profit_percent: takeProfitPercent\n}));\n\nconst hasPositionsToOpen = symbolsWithMarketValue.length > 0 && valuePerSymbol > 0;\n\nreturn [\n  {\n    json: {\n      positions_not_in_top_sentiment: positionsNotInTopSentiment,\n      symbols_not_in_open_positions: symbolsWithMarketValue,\n      has_positions_to_close: positionsNotInTopSentiment.length > 0,\n      has_positions_to_open: hasPositionsToOpen\n    }\n  }\n];"
      },
