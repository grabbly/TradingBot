    {
      "parameters": {
        "jsCode": "const sentimentItems = $items('get_sentiment_scores');\nconst emaItems = $items('get_latest_ema');\n\nconst emaMap = new Map(emaItems.map(item => [item.json.symbol, item.json]));\n\nconst withEmaFilter = sentimentItems\n  .filter(item => {\n    const ema = emaMap.get(item.json.stock);\n    if (!ema) return false;\n    if (ema.close_price == null || ema.ema9 == null || ema.ema21 == null) return false;\n    if (ema.rsi14 == null || ema.volume == null || ema.volume_ma20 == null) return false;\n    const emaOk = Number(ema.ema9) > Number(ema.ema21);\n    const rsiOk = Number(ema.rsi14) < 70;\n    const volumeOk = Number(ema.volume) > Number(ema.volume_ma20);\n    return emaOk && rsiOk && volumeOk;\n  })\n  .map(item => ({\n    json: {\n      ...item.json,\n      ema: emaMap.get(item.json.stock),\n      ema_filter_applied: true\n    }\n  }));\n\nconst baseList = withEmaFilter.length > 0 ? withEmaFilter : sentimentItems;\nconst sortedItems = baseList.sort((a, b) => b.json.sentimentScore - a.json.sentimentScore);\n\nconst maxPerSector = 2;\nconst sectorCounts = new Map();\nconst topItems = [];\n\nfor (const item of sortedItems) {\n  const sector = item.json.sector || 'Unknown';\n  const count = sectorCounts.get(sector) || 0;\n  if (count >= maxPerSector) continue;\n  topItems.push(item);\n  sectorCounts.set(sector, count + 1);\n  if (topItems.length >= 4) break;\n}\n\nreturn topItems"
      },
