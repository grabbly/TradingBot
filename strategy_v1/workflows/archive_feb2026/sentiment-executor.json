    {
      "parameters": {
        "jsCode": "const symbol = $('positions_to_open').item.json.symbol;\nconst notional = Number($('positions_to_open').item.json.market_value_per_symbol || 0);\nconst stopLossPercent = Number($('positions_to_open').item.json.stop_loss_percent || 0.02);\nconst takeProfitPercent = Number($('positions_to_open').item.json.take_profit_percent || 0.04);\n\nconst price = Number($json.trade?.p || $json.last?.p || $json.p || 0);\nif (!price || !notional) {\n  return { json: { symbol, qty: 0, notional, price, stop_price: null, take_profit: null } };\n}\n\nconst qty = Number((notional / price).toFixed(6));\nconst stop_price = Number((price * (1 - stopLossPercent)).toFixed(2));\nconst take_profit = Number((price * (1 + takeProfitPercent)).toFixed(2));\n\nreturn {\n  json: {\n    symbol,\n    qty,\n    notional,\n    price,\n    stop_price,\n    take_profit\n  }\n};"
      },
